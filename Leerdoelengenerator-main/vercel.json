{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "rewrites": [
    { "source": "/(?!api/).*", "destination": "/index.html" }
  ],
  "redirects": [
    {
      "source": "https://leerdoelen.digited.nl/(.*)",
      "destination": "https://leerdoelen.edwinspielhagen.nl/$1",
      "permanent": true
    }
  ]
}
