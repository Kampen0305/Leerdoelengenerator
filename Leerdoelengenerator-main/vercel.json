{
  "rewrites": [
    { "source": "/(?!api/).*", "destination": "/index.html" }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "leerdoelen.digited.nl" }
      ],
      "destination": "https://leerdoelen.edwinspielhagen.nl/$1",
      "permanent": true
    }
  ]
}
